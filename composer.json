{
    "name": "apphp/ai-with-php-examples",
    "description": "Code examples provided for the book Artificial Intelligence with PHP.",
    "type": "project",
    "license": "MIT",
    "version": "0.5.0",
    "packageGroups": {
        "system": [
            "php"
        ],
        "ml": [
            "php-ai/php-ml",
            "rubix/ml"
        ],
        "ml-math": [
            "sciphp/numphp",
            "numphp/numphp",
            "markrogoyski/math-php"
        ],
        "llm-models": [
            "llm-agents/agents",
            "openai-php/client"
        ],
        "markdown": [
            "league/commonmark"
        ]
    },
    "require": {
        "php": "~8.2.0",
        "php-ai/php-ml": "0.10.0",
        "rubix/ml": "~2.5.1",
        "sciphp/numphp": "0.4.0",
        "numphp/numphp": "v1.2.0",
        "markrogoyski/math-php": "~2.11.0",
        "llm-agents/agents": "~1.7.0",
        "openai-php/client": "~0.10.1",
        "symfony/http-client": "~7.2",
        "league/commonmark": "~2.6.1",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "app\\": ""
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
